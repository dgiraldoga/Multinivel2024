
SCHEMA 

CREATE SCHEMA mio AUTHORIZATION mio_cali;


TABLES 

CREATE TABLE mio.users (
user_id integer PRIMARY KEY NOT NULL,
name_user varchar (100),
lastname_user varchar (100),
city varchar (100),
status char(1),
user_regdate timestamp DEFAULT current_timestamp,
CHECK(status IN ('A', 'I'))
);

CREATE TABLE mio.applications (
app_id integer PRIMARY KEY NOT NULL,
app_desc varchar (100) NOT NULL, 
app_status char(1),
CHECK (app_status IN ('A', 'I'))
);

CREATE TABLE mio.cards (
serial_card integer PRIMARY KEY NOT NULL,
card_status char(1),
card_regdate timestamp DEFAULT current_timestamp,
app_id integer,
CHECK(card_status IN ('A', 'I')),
CONSTRAINT fk_app_id FOREIGN KEY (app_id) REFERENCES mio.applications (app_id)
);

CREATE TABLE mio.cards_x_users(

id serial PRIMARY KEY NOT NULL,
serial_card integer,
user_id integer,
crd_usr_status char(1),
crd_usr_regdate timestamp DEFAULT current_timestamp,
CHECK(crd_usr_status IN ('A', 'I')),
CONSTRAINT fk_user FOREIGN KEY (user_id) REFERENCES mio.users(user_id),
CONSTRAINT fk_card FOREIGN KEY (serial_card) REFERENCES mio.cards(serail_card)
);

CREATE TABLE mio.line_details(

ld_id integer PRIMARY KEY NOT null,
ld_desc varchar (100),
ld_status char(1),
ld_regdate timestamp DEFAULT current_timestamp,
CHECK(ld_status IN ('A', 'I'))
);

create table mio.transport_providers(
tp_id int primary key not null, 
tp_desc varchar (100),
tp_status chr(1),
tp_repdate timestamp default current_timestamp, 
check (tp_status in ('A', 'I'))
);

create table mio.validators(
vl_id int primary key not null, 
val_desc varchar(100),
val_status char(1),
val_regdate timestamp default current_timestamp,
CONSTRAINT fk_transport FOREIGN KEY (tp_id) REFERENCES mio.transport_providers(tp_id)

);


CREATE TABLE mio.cards_usages(
id serial PRIMARY KEY NOT NULL,
serial_card integer,
ld_id integer,
user_id integer,
vl_id integer,
cu_datetime timestamp,
fare_value integer NOT NULL, 
type_usage integer NOT NULL,
tsn integer NOT NULL,
CONSTRAINT fk_user FOREIGN KEY (user_id) REFERENCES mio.users(user_id),
CONSTRAINT fk_card FOREIGN KEY (serial_card) REFERENCES mio.cards(serail_card),
CONSTRAINT fk_line FOREIGN KEY (ld_id) REFERENCES mio.line_details(ld_id), 
CONSTRAINT fk_validators FOREIGN KEY (vl_id) REFERENCES mio.validators(vl_id)

)

